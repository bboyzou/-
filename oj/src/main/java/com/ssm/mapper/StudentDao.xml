<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssm.dao.StudentDao">
<select id="SearchStudent" resultType="String">
select sid
from student
where 
sid=#{sid}
and passw=#{password}
</select>

</mapper>